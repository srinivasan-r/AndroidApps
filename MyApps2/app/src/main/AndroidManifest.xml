<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.vs.myapps2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".RecyclerEmiActivity"
            android:label="@string/title_activity_scrolling"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity
            android:name=".EmiTableActivity"
            android:label="@string/title_activity_emi_table"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".PetrolVsDieselActivity"
            android:label="@string/title_activity_petrol_vs_diesel"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
    </application>

</manifest>